<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.beginner.entity.Friend" table="friend">
		<id name="id" type="java.lang.String">
			<column name="ID"></column>
			<generator class="com.hibernate.beginner.pkgenerator.CustomPrimaryKeyGenerator"></generator>	
	    </id>
	   
	   	<property name="name" type="java.lang.String">
	   		<column name="fullname"></column>
	   	</property>
	   	
	   	<property name="location" type="java.lang.String">
	   		<column name="friendLocation"></column>
	   	</property>
	   	
	  
	   
	</class>


</hibernate-mapping>